AutoHunt.Steps.4 := "Id: " ValidateJumpStep
AutoHunt.Steps.5 := "-"
AutoHunt.Steps.6 := "-"
AutoHunt.Steps.7 := "-"
AutoHunt.Steps.8 := "-"
case "Right Click":
AutoHunt.Steps.3 := StepAction
AutoHunt.Steps.4 := RightClickSQM
AutoHunt.Steps.5 := "-"
AutoHunt.Steps.6 := "-"
AutoHunt.Steps.7 := "-"
