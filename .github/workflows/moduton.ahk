LV_GetText(OutputVar, A_Index, 4)
AutoHunt.Steps[A_Index] := AutoHunt.Steps[A_Index] . "||" . OutputVar
LV_GetText(OutputVar, A_Index, 5)
AutoHunt.Steps[A_Index] := AutoHunt.Steps[A_Index] . "||" . OutputVar
LV_GetText(OutputVar, A_Index, 6)
